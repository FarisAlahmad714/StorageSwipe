{
  "expo": {
    "name": "StorageSwipe",
    "slug": "StorageSwipeApp",
    "scheme": "storageswipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fwayne714.storageswipe",
      "buildNumber": "11",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "StorageSwipe needs access to your photos to help you clean up your gallery.",
        "NSPhotoLibraryAddUsageDescription": "StorageSwipe needs permission to save photos and create albums.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fwayne714.storageswipe"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6814cc88-168c-4cbf-ad21-7a560b47fcc9"
      }
    }
  }
}
