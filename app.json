{
  "expo": {
    "name": "StorageSwipe",
    "slug": "StorageSwipeApp",
    "scheme": "storageswipe",
    "owner": "fingolfin-714",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/header.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "StorageSwipe needs access to your photos to help you clean up your gallery.",
          "savePhotosPermission": "StorageSwipe needs permission to save photos and create albums.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "expo-build-properties"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.fwayne714.storageswipe",
      "buildNumber": "21",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "StorageSwipe needs access to your photos to help you clean up your gallery.",
        "NSPhotoLibraryAddUsageDescription": "StorageSwipe needs permission to save photos and create albums.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fwayne714.storageswipe",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "99b625cf-180d-4d23-aac3-398da765a1ca"
      }
    }
  }
}
